{
  "name": "@villium/echo-fx",
  "version": "0.1.0",
  "description": "Real-time audio effects processing (filters, delays, distortion)",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "README.md", "LICENSE"],
  "author": "Villium",
  "sideEffects": false,
  "scripts": {
