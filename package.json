    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "tsup": "^8.0.0",
    "vitest": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/villium/echo-fx.git",
    "directory": "packages/pulse-effects"
  },
  "keywords": [
    "audio",
    "effects",
    "filters",
    "delay",
    "compressor",
    "biquad",
    "dsp"
